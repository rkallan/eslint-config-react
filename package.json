{
    "name": "eslint-config-react-rkallan",
    "version": "1.0.0",
    "description": "Eslint Rules React",
    "keywords": [
        "code standard",
        "es6",
        "javascript lint",
        "lint",
        "style guide",
        "react"
    ],
    "bugs": {
        "url": "https://github.com/rkallan/eslint-config-react/issues/new",
        "email": "r.kallan@upcmail.nl"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:rkallan/eslint-config-react.git"
    },
    "license": "MIT",
    "author": {
        "name": "Ravi Kallan",
        "email": "r.kallan@upcmail.nl"
    },
    "main": "index.js",
    "files": [
        "*.js",
        "!.*",
        "!*.config.*"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "scripts": {
        "build": "babel src -d build",
        "build:npm": "babel src -d .",
        "commit": "git-cz",
        "format": "prettier --write './**/*.js'",
        "lint": "eslint . --fix",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-rkallan": "^2.0.1",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^7.0.2",
        "object.assign": "^4.1.2",
        "prettier": "^2.4.1",
        "react": "^17.0.2",
        "rkallan-prettier-config": "^2.1.0",
        "semantic-release": "^19.0.3"
    },
    "peerDependencies": {
        "eslint": ">=7.32.0",
        "eslint-config-prettier": ">=8.3.0",
        "eslint-config-rkallan": ">=2.0.3",
        "eslint-plugin-jsx-a11y": ">=6.4.1",
        "eslint-plugin-prettier": ">=4.0.0",
        "eslint-plugin-react": ">=7.26.1",
        "eslint-plugin-react-hooks": ">=4.2.0",
        "object.assign": ">=4.1.2",
        "react": ">=17.0.2"
    },
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/changelog",
        "@semantic-release/github",
        "@semantic-release/npm",
        "@semantic-release/git"
    ]
}
