{
    "name": "eslint-config-react-rkallan",
    "version": "1.0.0",
    "description": "Eslint Rules React",
    "keywords": [
        "code standard",
        "es6",
        "javascript lint",
        "lint",
        "style guide",
        "react"
    ],
    "bugs": {
        "url": "https://github.com/rkallan/eslint-config-react/issues/new",
        "email": "r.kallan@upcmail.nl"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:rkallan/eslint-config-react.git"
    },
    "license": "MIT",
    "author": {
        "name": "Ravi Kallan",
        "email": "r.kallan@upcmail.nl"
    },
    "main": "index.js",
    "files": [
        "*.js"
    ],
    "scripts": {
        "build": "babel src -d build",
        "build:npm": "babel src -d .",
        "format": "prettier --write './**/*.js'",
        "lint": "eslint . --fix"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-rkallan": "^1.0.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-mdx": "^1.7.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react-hooks": "^4.0.4",
        "husky": "^4.2.5",
        "object.assign": "^4.1.0",
        "prettier": "^2.0.5",
        "react": "^16.13.1",
        "rkallan-prettier-config": "^2.0.0",
        "semantic-release": "^17.0.7"
    },
    "peerDependencies": {
        "eslint": ">= 6.8.0",
        "eslint-config-prettier": ">= 6.11.0",
        "eslint-config-rkallan": ">= 1.0.0",
        "eslint-plugin-jsx-a11y": ">= 6.2.3",
        "eslint-plugin-mdx": ">= 1.7.0",
        "eslint-plugin-prettier": ">= 3.1.3",
        "eslint-plugin-react-hooks": ">= 4.0.4",
        "object.assign": ">= 4.1.0",
        "react": ">= 16.13.1"
    },
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/changelog",
        "@semantic-release/github",
        "@semantic-release/npm",
        "@semantic-release/git"
    ]
}
